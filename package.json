{
  "name": "gps-to-gpx",
  "version": "2.0.0-dev",
  "description": "A simple tool that uses GPS waypoint data to generate GPX.",
  "keywords": [
    "gps",
    "gpx",
    "waypoints"
  ],
  "homepage": "https://github.com/pdhoopr/gps-to-gpx#readme",
  "bugs": "https://github.com/pdhoopr/gps-to-gpx/issues",
  "license": "MIT",
  "author": "Patrick Hooper <patrick.d.hooper@gmail.com> (https://patrickhooper.me)",
  "files": [
    "dist"
  ],
  "main": "dist/gps-to-gpx.js",
  "module": "dist/gps-to-gpx.esm.js",
  "browser": "dist/gps-to-gpx.umd.js",
  "repository": "github:pdhoopr/gps-to-gpx",
  "scripts": {
    "build": "microbundle",
    "check:format": "prettier --list-different '**/*.{js,json,md}'",
    "check:js": "eslint '**/*.js'",
    "format": "prettier --write '**/*.{js,json,md}'"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "microbundle": "^0.6.0",
    "prettier": "^1.14.0"
  },
  "engines": {
    "node": ">=6"
  },
  "eslintConfig": {
    "root": true,
    "plugins": [
      "import"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:import/recommended",
      "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true
      }
    },
    "env": {
      "es6": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,json,md}": [
      "prettier --write",
      "git add"
    ],
    "**/*.js": [
      "eslint"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
